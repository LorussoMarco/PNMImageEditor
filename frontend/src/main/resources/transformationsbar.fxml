<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="transformationsBar" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      prefWidth="150" spacing="10" alignment="CENTER"
      style="-fx-background-color: #F0F0F0; -fx-padding: 10;">

    <Button fx:id="bFlipUpDown" text="" prefWidth="120"/>
    <Button fx:id="bFlipSide" text="" prefWidth="120"/>
    <Button fx:id="bRotateC" text="" prefWidth="120" wrapText="true"/>
    <Button fx:id="bRotateAC" text="" prefWidth="120" wrapText="true"/>
    <Button fx:id="bNegative" text="" prefWidth="120"/>

</VBox>
