<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="pipelineVBox" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      prefWidth="200" spacing="10" alignment="CENTER"
      style="-fx-background-color: #F8F8F8; -fx-padding: 10;">

    <TextArea fx:id="pipelineTextArea" prefHeight="400" prefWidth="180" wrapText="true" editable="false"/>
    <Button fx:id="bApply" text="" prefWidth="150" alignment="CENTER"/>
    <Button fx:id="bClear" text="" prefWidth="150" alignment="CENTER"/>

</VBox>
